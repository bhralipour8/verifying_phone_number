#include<iostream>
using namespace std;
class Mobile {
public:
	char number[11];
	bool isValid()
	{
		if (number[0] == 0 && number[1] == 9)
		{
			return true;
		}
		else {
			return false;
		}
	}
};
int main()
{
	Mobile mobile;
	cout << "enter phone number: ";
	cin >> mobile.number;
	if (mobile.isValid)
	{
		cout << "good number!";
	}
	else {
		cout<< " wrong number!";
	}
}